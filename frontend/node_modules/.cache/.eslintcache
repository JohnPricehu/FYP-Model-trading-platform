                <Row>\n                  <Col>Total</Col>\n                  <Col>${order.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n\n              <ListGroup.Item>\n              {loadingPay && <Loading />}\n                {userInfo && !userInfo.isAdmin && !order.isPaid && (\n                  <ListGroup.Item>\n                    <Button\n                      type='button'\n                      className='btn btn-block'\n                      onClick={payHandler}\n                    >\n                      Pay Order\n                    </Button>\n                                        \n                  </ListGroup.Item>\n                )}\n                {userInfo && userInfo.isAdmin && !order.isPaid && (\n                  <ListGroup.Item>\n                    <Button\n                      type='button'\n                      className='btn btn-block'\n                      onClick={payHandler}\n                    >\n                      Mark As Paid\n                    </Button>\n                                        \n                  </ListGroup.Item>\n                )}\n                {loadingDeliver && <Loading />}\n                {order.isPaid && !order.isDelivered && (\n                  \n                  <ListGroup.Item>\n                <Button\n                      type='button'\n                      className='btn btn-block'\n                      onClick={deliverHandler}\n                    >\n                      Mark As Delivered\n                    </Button>\n                    </ListGroup.Item>\n                    )}\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  )\n}\n\nexport default OrderScreen\n","D:\\FYP demo\\frontend\\src\\actions\\orderAction.js",[],"D:\\FYP demo\\frontend\\src\\components\\CheckOutSteps.js",[],"D:\\FYP demo\\frontend\\src\\components\\Loader.js",[],"D:\\FYP demo\\frontend\\src\\component